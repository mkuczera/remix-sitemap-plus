{
  "name": "remix-sitemap-plus",
  "version": "1.0.0",
  "description": "",
  "source": "src/index.ts",
  "main": "./lib/commonjs/index.js",
  "module": "./lib/module/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/typescript/module/src/index.d.ts",
        "default": "./lib/module/index.js"
      },
      "require": {
        "types": "./lib/typescript/commonjs/src/index.d.ts",
        "default": "./lib/commonjs/index.js"
      }
    }
  },
  "scripts": {
    "build": "npm run build:commonjs && npm run build:module && npm run build:types",
    "build:commonjs": "babel src --out-dir lib/commonjs --extensions \".ts,.tsx\"",
    "build:module": "babel src --out-dir lib/module --extensions \".ts,.tsx\" --env-name build-module",
    "build:types": "tsc --declaration --emitDeclarationOnly --declarationDir lib/typescript",
    "clean": "rm -rf lib"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@types/glob": "^8.1.0",
    "@types/node": "^22.1.0",
    "typescript": "^5.5.4"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/parser": "^7.25.3"
  }
}
